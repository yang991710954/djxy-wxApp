.recordTem-container {
  background-color: #fff;
  margin-top: 10rpx;
  height: 200rpx;
  border-bottom: 1px solid #ebebeb;
}

.trainingPattern {
  padding: 0 30rpx;
  padding-bottom: 25rpx;
  border-bottom: 1px solid #ebebeb;
}

.trainingTimeBox {
  display: flex;
  flex-direction: row;
  padding: 20rpx 0;
  border-bottom: 1xp solid #eee;
}

.trainingTimeBox>text {
  flex: 1;
  color: #666;
  font-size: 26rpx;
  text-align: center;
}

.trainingTimeBox>text:nth-child(1) {
  text-align: left;
}

.trainingTimeBox>text:nth-child(2) {
  color: #19a2ff;
}

.trainingPath {
  font-size: 26rpx;
}

.TrainingScoreBox {
  display: flex;
  flex-direction: row;
  padding: 0 30rpx;
  padding-top: 12rpx;
}

.TrainingScoreBox>text, .TrainingScoreBox>view {
  flex: 1;
  font-size: 26rpx;
  text-align: center;
}

.TrainingScoreBox>text:nth-child(1) {
  text-align: left;
}

.enterArrow {
  width: 20rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-left: 25rpx;
}
